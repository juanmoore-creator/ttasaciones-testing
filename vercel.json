{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/((?!api/.*).*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/**/*": {
            "maxDuration": 60,
            "memory": 1024
        }
    }
}